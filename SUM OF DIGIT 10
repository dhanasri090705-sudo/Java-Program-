import java.io.*;
import java.util.*;
public class Solution{
    public static void main(String[] args){
        Scanner scanner = new Scanner(System.in);
        int num = scanner.nextInt();
        int org = num;
        int rem,sum=0;
        if(num>99){
            while(num>0){
                rem=num%10;
                sum=sum+rem;
                num=num/10;   
        }
            System.out.println("Sum of digit is "+sum);
        }
        else{
            System.out.println("Invalid Input");
        }
    }
}
