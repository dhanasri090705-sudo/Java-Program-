import java.io.*;
import java.util.*;
public class Solution {
    public static void main(String[] args){
        Scanner sc= new Scanner(System.in);
        int size=sc.nextInt();
        int arr[]= new int[size];
        float sum=0;
        float mean=0;
        for(int i=0;i<size;i++){
            arr[i]=sc.nextInt();
            sum=sum+arr[i];
        }
        mean=sum/size;
        System.out.printf("Array Mean Value is %.2f",mean);
    }
}
