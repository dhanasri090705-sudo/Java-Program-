import java.io.*;
import java.util.*;
public class Solution{
    public static void main(String[] args){
        Scanner scanner = new Scanner(System.in);
        int a = scanner.nextInt(),sum=0;
        for(int i=1;i<a;i++){
            if(a%i==0){
                sum=sum+i;  
            }
        }
            if(a<sum){
                System.out.println("Abundant Number");
            }
            else{
                System.out.println("Not Abundant Number");
            }          
        }
}
    
